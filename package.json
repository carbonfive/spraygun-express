{
  "name": "app-prototype",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "prestart": "node bin/pre-start",
    "start": "nodemon --watch . --watch .env.development --watch .env.development.local app/server.js",
    "lint": "eslint 'app/**/*.js'",
    "server": "node app/server.js",
    "test": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "app/**/*.js": "eslint"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.10",
    "module-alias": "^2.2.0",
    "morgan": "^1.9.0",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "shelljs": "^0.8.2",
    "supertest": "^6.1.6"
  },
  "engines": {
    "node": "^16.13.1",
    "yarn": ">=1.12.1"
  },
  "_moduleAliases": {
    "@app": "app"
  },
  "jest": {
    "roots": [
      "app"
    ],
    "moduleNameMapper": {
      "^@app(.*)$": "<rootDir>/app$1"
    },
    "collectCoverageFrom": [
      "app/**/*.js"
    ]
  }
}
