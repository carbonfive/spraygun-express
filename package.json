{
  "name": "app-prototype",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prestart": "node bin/pre-start",
    "start": "nodemon --watch . --watch .env.development --watch .env.development.local app/server.js",
    "lint": "eslint 'app/**/*.js'",
    "server": "node app/server.js",
    "test": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "app/**/*.js": "eslint"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.3",
    "express-basic-auth": "^1.1.6",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.10",
    "module-alias": "^2.2.0",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "dotenv": "^7.0.0",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.3.2",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.1",
    "jest": "^24.5.0",
    "jest-junit": "^6.3.0",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "shelljs": "^0.8.2",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": "10.15.0",
    "yarn": ">=1.12.1"
  },
  "_moduleAliases": {
    "@app": "app"
  },
  "jest": {
    "roots": [
      "app"
    ],
    "moduleNameMapper": {
      "^@app(.*)$": "<rootDir>/app$1"
    },
    "collectCoverageFrom": [
      "app/**/*.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
