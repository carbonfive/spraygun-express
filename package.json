{
  "name": "app-prototype",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "prestart": "node bin/pre-start",
    "start": "ts-node-dev --transpile-only --watch .,.env.development,.env.development.local app/server.ts",
    "lint": "eslint 'app/**/*.{js,ts}'",
    "server": "ts-node --transpile-only app/server.ts",
    "test": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "app/**/*.js": "eslint"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.10",
    "module-alias": "^2.2.0",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^6.0.0",
    "jest": "^27.0.1",
    "jest-junit": "^12.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "shelljs": "^0.8.2",
    "supertest": "^6.1.3",
    "ts-node-dev": "^1.1.6"
  },
  "engines": {
    "node": "^14.16.1",
    "yarn": ">=1.12.1"
  },
  "_moduleAliases": {
    "@app": "app"
  },
  "jest": {
    "roots": [
      "app"
    ],
    "moduleNameMapper": {
      "^@app(.*)$": "<rootDir>/app$1"
    },
    "collectCoverageFrom": [
      "app/**/*.{js,ts}"
    ]
  }
}
