{
  "name": "app-prototype",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prestart": "node bin/pre-start",
    "start": "nodemon --watch . --watch .env.development --watch .env.development.local app/server.js",
    "lint": "eslint 'app/**/*.js'",
    "server": "node app/server.js",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "app/**/*.js": "eslint"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "helmet": "^3.12.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "module-alias": "^2.1.0",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "dotenv": "^6.0.0",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "jest-junit": "^5.1.0",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.13.7",
    "shelljs": "^0.8.2",
    "supertest": "^3.1.0"
  },
  "engines": {
    "node": "10.14.2",
    "yarn": ">=1.12.1"
  },
  "_moduleAliases": {
    "@app": "app"
  },
  "jest": {
    "roots": [
      "app"
    ],
    "moduleNameMapper": {
      "^@app(.*)$": "<rootDir>/app$1"
    },
    "collectCoverageFrom": [
      "app/**/*.js"
    ]
  }
}
